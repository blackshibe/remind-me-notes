{
	"expo": {
		"name": "remind-me",
		"slug": "remind-me",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#FFFFFF"
			},
			"permissions": ["VIBRATE", "WRITE_EXTERNAL_STORAGE", "READ_EXTERNAL_STORAGE", "SCHEDULE_EXACT_ALARM"],
			"package": "com.blackshibe.reminders"
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"extra": {
			"eas": {
				"projectId": "4db701ef-6737-4556-8761-29c727bb665e"
			}
		},
		"plugins": [
			[
				"expo-build-properties",
				{
					"android": {
						"kotlinVersion": "1.7.10"
					}
				}
			]
		]
	}
}
